<html>
    <head>
        <meta charset="utf-8"/>
        <title>EJS Example</title>
    </head>
    <body>
        <h1> <%= title %></h1>
        <% animals.forEach( (item) => {%>
        <figure>
            <img src="<%=  item.image %>" width="300px">
            <br/>
            <figurecaption>
                <%= item.imgtitle %>
            </figurecaption>
        </figure>
        <% }); %>
    </body>
</html>